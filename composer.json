{
    "name": "mikail/vibeui",
    "description": "A clean, responsive Laravel/Livewire UI component library that uses TailwindCSS.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "VibeUI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VibeUI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VibeUI\\VibeUIServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^12.0",
        "blade-ui-kit/blade-icons": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

